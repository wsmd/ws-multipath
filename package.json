{
  "name": "ws-multipath",
  "version": "0.1.0-beta.0",
  "description": "Serve multiple WebSocket endpoints using one shared server",
  "main": "index.js",
  "engines": {
    "node": ">=6.2.2"
  },
  "repository": "wsmd/ws-multipath",
