  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wsmd/ws-multipath/issues"
  },
  "homepage": "https://github.com/wsmd/ws-multipath#readme",
  "keywords": [
    "ws",
    "websocket",
    "multipath",
    "server"
  ],
  "jest": {
    "testRegex": "/test/.*.test.js$"
